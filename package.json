    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "@tailwindcss/custom-forms": "^0.2.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "html-webpack-plugin": "^3.2.0",
    "luxon": "^1.22.0",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-footnote": "^3.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.27",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "purgecss": "^2.1.0",
    "sass-loader": "^8.0.2",
    "stimulus": "^1.1.1",
    "style-loader": "^1.1.3",
    "tailwindcss": "^1.2",
    "turbolinks": "^5.2.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "scripts": {
    "build": "run-s -l clean package:prod generate purge",
    "build:serve": "run-s -l clean package:prod generate purge serve:dist",
    "clean": "rm -rf dist",
    "clean:assets": "rm -rf dist/assets",
    "dev": "run-p -l package serve",
    "generate": "eleventy",
    "package": "webpack --mode development --watch --env.NODE_ENV=development",
    "package:prod": "webpack --mode production --env.NODE_ENV=production",
    "purge": "purgecss --config purgecss.config.js --output dist/assets",
    "serve": "eleventy --serve",
    "serve:dist": "npx http-server dist"
  },
  "dependencies": {}
}
